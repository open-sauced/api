{
  "name": "@open-sauced/api",
  "version": "1.57.0-beta.2",
  "keywords": [],
  "description": "API dot Open Sauced is NestJS and SupaBase powered OAS3 backend designed to remove client complexity and provide a structured graph of all @open-sauced integrations",
  "author": "TED Vortex <ted.vortex@gmail.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "license": "MIT",
  "homepage": "https://github.com/open-sauced/api.opensauced.pizza#readme",
  "bugs": {
    "url": "https://github.com/open-sauced/api.opensauced.pizza/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-sauced/api.opensauced.pizza.git"
  },
  "engines": {
    "node": ">= 16.15",
    "npm": ">= 8.12"
  },
  "scripts": {
    "types:auto-add": "npx typesync",
    "types:auto-check": "npm run types:auto-add -- --dry",
    "push": "npx @open-sauced/conventional-commit",
    "nest": "npx nest",
    "start": "npm run nest -- start",
    "start:dev": "npm run start -- --watch",
    "start:debug": "npm run start:dev -- --debug",
    "prebuild": "npx rimraf dist",
    "build": "npm run nest -- build",
    "build:package": "npx tsc -p tsconfig.package.json",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "npm run lint -- --fix",
    "format": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run test --config test/jest-e2e.json",
    "start:prod": "node dist/main",
    "docs": "npx compodoc -p tsconfig.json --hideGenerator --disableDependencies -d ./dist/documentation ./src",
    "docs:serve": "npm run docs -- --serve",
    "generate:swagger": "npm run nest -- start --config nest-cli.generate-swagger.json"
  },
  "dependencies": {
    "@fastify/express": "^2.3.0",
    "@fastify/helmet": "^10.1.0",
    "@fastify/rate-limit": "^8.0.0",
    "@fastify/static": "^6.10.1",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-fastify": "^9.4.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/terminus": "^9.2.2",
    "@nestjs/typeorm": "^9.0.1",
    "@supabase/supabase-js": "^1.35.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "nestjs-pino": "^3.2.0",
    "nestjs-supabase-auth": "^1.0.9",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.10.0",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0",
    "rxjs": "^7.8.1",
    "semver": "^7.5.0",
    "stripe": "^11.17.0",
    "tier": "^5.10.0",
    "typeorm": "^0.3.15",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@darraghor/eslint-plugin-nestjs-typed": "^3.20.5",
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@open-sauced/check-engines": "^1.2.0",
    "@open-sauced/conventional-commit": "^1.0.1",
    "@swc/core": "^1.3.56",
    "@types/eslint": "^8.37.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.16.3",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/pg": "^8.6.6",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.13",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.15",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "29.6.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "typesync": "^0.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      ".module.ts",
      ".dto.ts",
      ".config.ts",
      ".entity.ts",
      ".constants.ts",
      "index.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
