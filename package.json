{
  "name": "@open-sauced/api.opensauced.pizza",
  "version": "1.25.0",
  "keywords": [],
  "description": "API dot Open Sauced is NestJS and SupaBase powered OAS3 backend designed to remove client complexity and provide a structured graph of all @open-sauced integrations",
  "author": "TED Vortex <ted.vortex@gmail.com>",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/open-sauced/api.opensauced.pizza#readme",
  "bugs": {
    "url": "https://github.com/open-sauced/api.opensauced.pizza/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-sauced/api.opensauced.pizza.git"
  },
  "engines": {
    "node": ">= 16.15",
    "npm": ">= 8.12"
  },
  "scripts": {
    "types:auto-add": "npx typesync",
    "types:auto-check": "npm run types:auto-add -- --dry",
    "push": "npx @open-sauced/conventional-commit",
    "supabase": "supabase",
    "db:changes": "npm run supabase -- db changes",
    "db:commit": "npm run supabase -- db commit",
    "db:push": "npm run supabase -- db push",
    "db:reset": "npm run supabase -- db reset",
    "db:start": "npm run supabase -- start",
    "db:stop": "npm run supabase -- stop",
    "nest": "npx nest",
    "start": "npm run nest -- start",
    "start:dev": "npm run start -- --watch",
    "start:debug": "npm run start:dev -- --debug",
    "prebuild": "rimraf dist",
    "build": "npm run nest -- build",
    "eslint": "npx eslint",
    "lint": "npm run eslint -- . --ext .ts,.js",
    "format": "npm run lint -- --fix",
    "test": "echo 'Error: test configuration not specified' && exit 0",
    "test:watch": "npm run test --watch",
    "test:cov": "npm run test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run test --config test/jest-e2e.json",
    "start:prod": "node dist/main",
    "docs": "npx compodoc -p tsconfig.json --hideGenerator --disableDependencies -d ./dist/documentation ./src",
    "docs:serve": "npm run docs -- --serve"
  },
  "dependencies": {
    "@fastify/express": "^2.3.0",
    "@fastify/helmet": "^10.1.0",
    "@fastify/rate-limit": "^7.6.0",
    "@fastify/static": "^6.8.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.3.1",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.1",
    "@nestjs/passport": "^9.0.1",
    "@nestjs/platform-fastify": "^9.3.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@supabase/supabase-js": "^1.35.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "nestjs-pino": "^3.1.2",
    "nestjs-supabase-auth": "^1.0.9",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.9.0",
    "pino-http": "^8.3.2",
    "pino-pretty": "^9.1.1",
    "rxjs": "^7.8.0",
    "semver": "^7.3.8",
    "stripe": "^11.8.0",
    "typeorm": "^0.3.11",
    "typeorm-transactional-cls-hooked": "^0.1.21"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@darraghor/eslint-plugin-nestjs-typed": "^3.18.0",
    "@nestjs/cli": "^9.1.9",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.1",
    "@open-sauced/check-engines": "^1.2.0",
    "@open-sauced/conventional-commit": "^1.0.1",
    "@swc/core": "^1.3.32",
    "@types/eslint": "^8.21.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/pg": "^8.6.6",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.13",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.11",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "29.4.1",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.7.4",
    "typesync": "^0.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
